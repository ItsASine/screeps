{
  "extends": "google",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-var": "off",
    "comma-dangle": "off",
    "semi": "error",
    "curly": "error",
    "quotes": ["error", "single"],
    "newline-after-var": "error"
  }
}